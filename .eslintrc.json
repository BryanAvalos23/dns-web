{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "no-console": "warn",
    "no-unused-vars": "warn",
    "indent": [
      "error",
      2
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}